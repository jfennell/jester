{
  "manifest_version": 2,

  "name": "Jester",
  "description": "Easy navigation of webcomics.",
  "version": "0.1",

  "web_accessible_resources": ["fairlocal.js"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["injector.js"]
      // "js": ["lib/jquery.min.js", "fairlocal.js"]
      // "js": ["lib/mootools-yui-compressed.js", "fairlocal.js"]
    }
  ],
  "permissions": [
    "tabs", "https://www.google.com/*"
  ]
}
