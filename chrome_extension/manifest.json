{
  "manifest_version": 2,

  "name": "Jester",
  "description": "Easy navigation of webcomics.",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
	    "http://www.girlgeniusonline.com/comic.php*",
		"http://www.cad-comic.com/*",
		"http://www.penny-arcade.com/*",
		"http://*.xkcd.com/*"
	  ],
      "js": [
				"lib/jquery-1.9.1.min.js",
				"lib/jsuri-1.1.1.min.js",
				"jester.js"
			]
    }
  ],
  "permissions": [
    "tabs",
	"http://www.girlgeniusonline.com/*",
	"http://www.cad-comic.com/*",
	"http://www.penny-arcade.com/*",
	"http://*.xkcd.com/*"
  ]
}
